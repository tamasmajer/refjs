{
  "name": "magic-box",
  "version": "1.0.0",
  "description": "Think in boxes. A tiny (<2.5KB) library for building reactive UIs by composing simple HTML containers.",
  "main": "magic-box.min.js",
  "module": "magic-box.js",
  "type": "module",
  "scripts": {
    "minify": "uglifyjs magic-box.js --compress --mangle --mangle-props reserved=['SIGNAL','bind','tags','dom','ui','rawVal','val','oldVal','Content-Type','isConnected','nodeType','box'] --toplevel -o magic-box.min.js",
    "size": "node -e \"const fs=require('fs'),zlib=require('zlib');const min=fs.readFileSync('magic-box.min.js');console.log('Minified:',min.length,'bytes');console.log('Gzipped:',zlib.gzipSync(min).length,'bytes');\"",
    "build": "npm run minify && npm run size"
  },
  "keywords": [
    "box",
    "magic-box",
    "magicbox",
    "box.js",
    "ui",
    "library",
    "tiny",
    "vanjs",
    "reactive"
  ],
  "devDependencies": {
    "uglify-js": "^3.19.3"
  }
}